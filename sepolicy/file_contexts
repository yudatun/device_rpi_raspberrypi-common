########################################
# Devices
#
/dev/block/mmcblk0         u:object_r:mmcblk0_block_device:s0
/dev/block/mmcblk0p1       u:object_r:mmcblk0_block_device:s0
/dev/block/mmcblk0p2       u:object_r:mmcblk0_block_device:s0

########################################
# Boot files
#
/boot(/.*)?        u:object_r:boot_file:s0

#############################
# lib files
#
/lib(/.*)?    u:object_r:system_file:s0
